{
    "spaceUnits": 2,
    "closeTags": {
        "whenOpening": false,
        "whenClosing": true
    },
    "jslint.options": {
        "white": true,
        "devel": true
    },
    "proxy": "http://username:password@proxy.com:8080",
    "path": {
        "**.css": {
            "useTabChar": false,
            "tabSize": 2,
            "linting.enabled": true,
            "spaceUnits": 2,
            "brackets-minifier.on-save": true
        },
        "**.js": {
            "insertHintOnTab": true,
            "scrollPastEnd": true,
            "linting.collapsed": false,
            "linting.enabled": true,
            "brackets-file-tree-exclude.excludeList": [
                "/.git/",
                "/dist/",
                "/bower_components/",
                "/node_modules/"
            ]
        }
    },
    "language": {
        "javascript": {
            "linting.prefer": [
                "JSLint"
            ],
            "linting.usePreferredOnly": false
        }
    },
    "language.fileExtensions": {
        "foo": "javascript"
    },
    "language.fileNames": {
        "pavement": "python"
    },
    "brackets-minifier.on-save-project": false,
    "brackets-minifier.js-project-minify": true,
    "brackets-minifier.css-project-minify": true,
    "brackets-minifier.js-custom-path": "",
    "brackets-minifier.css-custom-path": "",
    "brackets-minifier.project-exclude": "",
    "brackets-minifier.concat-js-filename": "",
    "brackets-minifier.concat-css-filename": "",
    "brackets-minifier.js-mangle": true,
    "brackets-minifier.js-compress": true
}